      program hello
      
      real, dimension (:,:), allocatable :: upper
      real, dimension (:,:), allocatable :: lower
      integer :: N
      integer :: i, j
      integer :: temp
      
      external DPOTRF
      
      N = 16
      
      allocate ( upper(N, N) )
      allocate ( lower(N, N) )
      
      do i = 1, N
         do j = 1, N
            if (j <= i) then
               temp = rand()
               lower(i, j) = temp
               upper(j, i) = temp
            else
               
lower(i, j) = temp
               upper(j, i) = temp
         end do
      end do



      Print *, "Hello World!"
      end program Hello
